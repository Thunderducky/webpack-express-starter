{
  "name": "webpack-express-starter",
  "author": "Eric Scott",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore client/*\" \"npm run webpack:dev\"",
    "webpack:production": "webpack --mode:production -w",
    "webpack:dev": "webpack --mode:development -w",
    "build": "npx webpack:production",
    "heroku-postbuild": "npm run build",
    "test": "jest client/src/",
    "lint": "eslint client/src"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.15.0",
    "eslint-plugin-jest": "^22.3.0",
    "jest": "^23.0.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "express": "^4.16.3"
  }
}
